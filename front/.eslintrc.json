{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "react-app",
        "react-app/jest",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["./src/assets"],
    "rules": {
        "max-classes-per-file": "off",
        "no-empty": "off",
        "no-use-before-define": "off",
        "react/prop-types": "off",
        "react/jsx-no-constructed-context-values": "off",
        "import/no-unresolved": "off",
        "jsx-a11y/img-redundant-alt": "off"
        
    }
}
